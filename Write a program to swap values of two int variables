// Write a program to swap values of two int variables 

#include<stdio.h>
#include<conio.h>

    void main()
{
        int a,b,c;
        printf("Enter in a or b ");
        scanf("%d%d",&a,&b);                
        c=a,a=b,b=c;
        printf("After swapping a = %d and b = %d",a,b);
    getch();
}
