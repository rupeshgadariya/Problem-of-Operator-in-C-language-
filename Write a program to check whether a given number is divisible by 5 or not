// Write a program to check whether a given number is divisible by 5 or not

#include<stdio.h>
#include<conio.h>

    void main()
{
        int x;
        printf("Enter a number ");
        scanf("%d",&x);
        printf( (x%5==0)? "It is divisible by 5" : "It is not divisible by 5" );
        
    getch();
}
