/* Write a program to take a three digit number from the user and rotate its digits
   by one position towards the right. */

#include<stdio.h>
#include<conio.h>

    void main()
{
        int x;                                                            
        printf("Enter a three digit number ");
        scanf("%d",&x);
      
        x= x%10 *100+ x/10;
 
         printf("Rotate its digits by one position towards the right :\n%d",x);
    getch();
}
