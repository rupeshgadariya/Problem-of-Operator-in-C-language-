// Write a program to print greater between two numbers. Print one number if both 
//   are the same.

#include<stdio.h>
#include<conio.h>

    void main()
{
         int x,y;
        printf("Enter a number ");
        scanf("%d%d",&x,&y);
        if (x>y)
            printf("%d is greater",x);
        else if (x<y)
            printf("%d is greater",y);
         else printf("%d Both are same",x);
      
    getch();
}

