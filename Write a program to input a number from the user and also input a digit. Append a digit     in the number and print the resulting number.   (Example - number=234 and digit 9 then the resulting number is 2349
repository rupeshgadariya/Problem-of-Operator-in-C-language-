/* Write a program to input a number from the user and also input a digit. Append a digit 
   in the number and print the resulting number.
  (Example - number=234 and digit 9 then the resulting number is 2349) */

  #include<stdio.h>
  #include<conio.h>
  
    void main()
  {
        int num, digit,x;
        printf("Enter the number ");
        scanf("%d",&num);
        printf("Enter single digit ");
        scanf("%d",&digit);
        
         x = num * 10 + digit ;
      printf("The resulting number is %d",x);    
    getch();
  }
