// Write a program to swap values of two int variables without using third variable and
//  arithmetic operators. 

#include<stdio.h>
#include<conio.h>

    void main()
{
        int a,b;
        printf("Enter in a or b ");
        scanf("%d%d",&a,&b);
        a=a^b;
        b=a^b;
        a=a^b;
       printf("%d",a);
       printf("\n%d",b);

        
    getch();
}
