// Write a program to check whether a given number is an even number or an odd number.

#include<stdio.h>
#include<conio.h>

    void main()
{
        int x;
        printf("Enter a number ");
        scanf("%d",&x);
       
       printf( (x%2==0)? "Even number": "Odd number "); 
        
    getch();
}
