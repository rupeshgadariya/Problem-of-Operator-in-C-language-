// Write a program to swap values of two int variables without using a third variable. 

#include<stdio.h>
#include<conio.h>

    void main()
{
        int a,b;
        printf("Enter in a or b ");
        scanf("%d%d",&a,&b);                
        a=a+b;
        b=a-b;
        a=a-b;

        printf("After swapping a = %d and b = %d",a,b);
    getch();
}
