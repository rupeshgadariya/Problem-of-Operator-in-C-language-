// Write a program to swap values of two int variables in single line arithmetic expression.

#include<stdio.h>
#include<conio.h>

    void main()
{
        int a,b;
        printf("Enter in a or b ");
        scanf("%d%d",&a,&b);                
        
       a += b -( b=a );

        printf("After swapping a = %d and b = %d",a,b);
    getch();
}
